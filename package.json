  },
  "homepage": "https://github.com/elvisperezgtz/logiscore#readme",
  "dependencies": {
    "cypress": "^8.2.0"
  },
  "devDependencies": {
    "@cypress/mocha-teamcity-reporter": "^1.0.0",
    "cypress-cucumber-preprocessor": "^4.2.0",
    "cypress-mochawesome-reporter": "^2.2.1",
    "mocha": "^9.0.3",
    "mocha-junit-reporter": "^2.0.0",
